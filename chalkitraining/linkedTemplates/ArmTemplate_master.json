{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "chalkitraining"
		},
		"AzureDataLakeStorageJSON_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageJSON'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"UIO Mongo VD registrations_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'UIO Mongo VD registrations'"
		},
		"kpireporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'kpireporting'"
		},
		"AzureDataLakeStorageJSON_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://chalkiblob.dfs.core.windows.net"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "d:\\\\PanEToshiko_DQ_UIO_REG.csv"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Niko"
		},
		"UIO Mongo VD registrations_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "heroku_app35158283"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "chalkitraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageJSON_accountKey": {
						"value": "[parameters('AzureDataLakeStorageJSON_accountKey')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"UIO Mongo VD registrations_connectionString": {
						"value": "[parameters('UIO Mongo VD registrations_connectionString')]"
					},
					"kpireporting_connectionString": {
						"value": "[parameters('kpireporting_connectionString')]"
					},
					"AzureDataLakeStorageJSON_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageJSON_properties_typeProperties_url')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"UIO Mongo VD registrations_properties_typeProperties_database": {
						"value": "[parameters('UIO Mongo VD registrations_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}